{
    "config": {
        "main-stack-size": {
            "value": 4000
        },
        "wifi-ssid": {
            "help": "WiFi SSID",
            "value": "\"SSID\""
        },
        "wifi-password": {
            "help": "WiFi Password",
            "value": "\"Password\""
        },
        "swo-enabled": {
            "help"      : "Enable single wire output disabling UART/stdio",
            "options"   : [null, 1],
            "value"     : null
          }
    },
    "target_overrides": {
        "*": {
            "platform.stdio-convert-newlines": true,
            "esp8266.provide-default" : true
        },
        "UBLOX_EVK_ODIN_W2": {
            "target.device_has": ["EMAC"]
        },
        "NUCLEO_L476RG": {
            "wifi-tx": "D8",
            "wifi-rx": "D2"
        },
        "NUCLEO_F401RE": {
            "wifi-tx": "D8",
            "wifi-rx": "D2"
        },
        "MM": {
            "esp8266.tx": "WIFI_CELLULAR_TX",
            "esp8266.rx": "WIFI_CELLULAR_RX",
            "esp8266.rts": "NC",
            "esp8266.cts": "NC",
            "esp8266.debug": true,
            "target.stdio_uart_tx": "MAIN_TX",
            "target.stdio_uart_rx": "MAIN_RX"
        }
    }
}
