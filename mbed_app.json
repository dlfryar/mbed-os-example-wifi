{
    "config": {
        "wifi-ssid": {
            "help": "WiFi SSID",
            "value": "\"SSID\""
        },
        "wifi-password": {
            "help": "WiFi Password",
            "value": "\"PASSWORD\""
        }
    },
    "target_overrides": {
        "*": {
            "platform.default-serial-baud-rate": 115200,
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-convert-newlines": true
        },
        "MM": {
            "target.stdio_uart_tx": "MAIN_TX",
            "target.stdio_uart_rx": "MAIN_RX"    
        }
    }
}
